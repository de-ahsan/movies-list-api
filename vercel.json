{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "src/index.ts"
      }
    ],
    "build": {
      "env": {
        "MONGODB_URI": "@MONGODB_URI"
      },
      "commands": {
        "postbuild": "npx ts-node src/db/userSeeder.ts"
      }
    }
  }
  